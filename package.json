{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Quick setup React project with Webpack 2, Babel 6, react-router 2, redux and redux-saga.",
  "scripts": {
    "lint": "eslint src && stylelint src/**/*.css",
    "clean": "rm -rf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:webpack",
    "prestart": "npm run lint",
    "start": "node devServer.js"
  },
  "author": "xingkui wang <bit3725@gmail.com> (http://github.com/bit3725)",
  "license": "ISC",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "redux"
  ],
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "core-decorators": "^0.11.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.15.0-alpha.1",
    "react-dom": "^0.15.0-alpha.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.0",
    "redux-saga": "^0.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-native-modules": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.9.0",
    "redbox-react": "^1.2.2",
    "style-loader": "^0.13.0",
    "stylelint": "^4.4.0",
    "stylelint-config-standard": "^3.0.0",
    "webpack": "^2.1.0-beta.4",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
